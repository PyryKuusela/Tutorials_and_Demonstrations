 RowBox[{
  RowBox[{"Solve", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"Expand", "[", 
      RowBox[{
       RowBox[{"CoefficientList", "[", 
        RowBox[{
         RowBox[{"Coefficient", "[", 
          RowBox[{
           RowBox[{"Expand", "[", 
            RowBox[{
             FractionBox["1", 
              SuperscriptBox["\[CurlyPhi]", "n"]], 
             RowBox[{"\[ScriptCapitalL]\[CurlyPhi]", "[", 
              RowBox[{"Sum", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{
                   RowBox[{
                    RowBox[{"Log", "[", "\[CurlyPhi]", "]"}], "*", 
                    RowBox[{"a", "[", 
                    RowBox[{"0", ",", 
                    RowBox[{"n", "+", "i"}]}], "]"}]}], "+", 
                   RowBox[{"a", "[", 
                    RowBox[{"1", ",", 
                    RowBox[{"n", "+", "i"}]}], "]"}]}], ")"}], "*", 
                 SuperscriptBox["\[CurlyPhi]", 
                  RowBox[{"n", "+", "i"}]]}], ",", 
                RowBox[{"{", 
                 RowBox[{"i", ",", "0", ",", "30"}], "}"}]}], "]"}], "]"}]}], 
            "]"}], ",", 
           SuperscriptBox["\[CurlyPhi]", "15"]}], "]"}], ",", 
         RowBox[{"Log", "[", "\[CurlyPhi]", "]"}]}], "]"}], "/.", 
       RowBox[{"n", "->", 
        RowBox[{"n", "-", "15"}]}]}], "]"}], "==", "0"}], ",", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"a", "[", 
       RowBox[{"0", ",", "n"}], "]"}], ",", 
      RowBox[{"a", "[", 
       RowBox[{"1", ",", "n"}], "]"}]}], "}"}]}], "]"}], "//", 
  "Simplify"}]}], "Input",
 CellChangeTimes->{{3.885302822762637*^9, 3.8853028366818733`*^9}, {
