3dff633b8c69"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Solve", "[", 
  RowBox[{
   RowBox[{
    RowBox[{"Coefficient", "[", 
     RowBox[{
      RowBox[{"Sum", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"S", "[", "i", "]"}], "*", 
         RowBox[{
          SubscriptBox["\[Theta]", "\[CurlyPhi]"], "[", 
          RowBox[{
           RowBox[{"f", "[", "\[CurlyPhi]", "]"}], ",", "i"}], "]"}]}], ",", 
